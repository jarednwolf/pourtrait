{
  "name": "pourtrait",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "health-check": "./scripts/health-check.sh",
    "validate-task": "./scripts/task-validation.sh",
    "pre-task": "npm run health-check",
    "post-task": "npm run health-check",
    "upgrade-check": "npm outdated && npm audit",
    "full-validation": "npm run type-check && npm run lint && npm test && npm run build",
    "setup:services": "./scripts/setup-services.sh",
    "setup:existing": "./scripts/connect-existing-services.sh",
    "setup:supabase": "./scripts/setup-services.sh --supabase",
    "setup:vercel": "./scripts/setup-services.sh --vercel",
    "setup:env": "./scripts/setup-services.sh --env",
    "sync:env": "./scripts/sync-env-to-vercel.sh",
    "deploy:production": "./scripts/production-deploy.sh",
    "verify:production": "./scripts/production-verification.sh",
    "monitor:production": "./scripts/production-monitoring.sh",
    "monitor:once": "./scripts/production-monitoring.sh --once",
    "backup:database": "npx supabase db dump --file backup-$(date +%Y%m%d_%H%M%S).sql",
    "migrate:production": "npx supabase db push",
    "rollback:deployment": "vercel rollback",
    "security:audit": "npm audit && npm run lint:security",
    "lint:security": "eslint . --ext .ts,.tsx --config .eslintrc.security.json",
    "performance:analyze": "npm run build && npx @next/bundle-analyzer",
    "db:reset": "npx supabase db reset",
    "db:migrate": "npx supabase migration new",
    "env:check": "node -e \"console.log('Environment check passed')\" && echo 'All required environment variables are set'"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.3.3",
    "@heroicons/react": "^2.2.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.38.5",
    "@types/web-push": "^3.6.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.3",
    "multer": "^2.0.2",
    "next": "^15.5.4",
    "next-pwa": "^5.6.0",
    "openai": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.34.4",
    "tailwind-merge": "^2.2.0",
    "web-push": "^3.6.7",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@esbuild/darwin-arm64": "^0.25.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/nextjs": "^9.1.10",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-dropzone": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-storybook": "^9.1.10",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "storybook": "^9.1.10",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
